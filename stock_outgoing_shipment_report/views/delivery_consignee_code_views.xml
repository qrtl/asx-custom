<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_delivery_consignee_code_tree" model="ir.ui.view">
        <field name="name">delivery.consignee.code.tree</field>
        <field name="model">delivery.consignee.code</field>
        <field name="arch" type="xml">
            <tree string="Delivery Consignee Code" editable="top">
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="action_delivery_consignee_code" model="ir.actions.act_window">
        <field name="name">Delivery Consignee Code</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">delivery.consignee.code</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
    </record>

    <menuitem action="action_delivery_consignee_code" id="menu_action_delivery_consignee_code" parent="delivery.menu_delivery" sequence="3"/>

</odoo>
